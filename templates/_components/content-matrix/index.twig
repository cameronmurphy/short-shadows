{% for block in props.matrix.all %}
  {% if block.status == 'enabled' %}
    {% switch block.type %}
      {% case 'releaseBlock' %}
        {% include '_components/release-block' with { props: block } only %}
      {% case 'contactBlock' %}
        {% include '_components/contact-block' with { props: block } only %}
      {% case 'galleryBlock' %}
        {% include '_components/gallery-block' with { props: block } only %}
      {% case 'gigsBlock' %}
        {% include '_components/gigs-block' with { props: block } only %}
      {% case 'headingBlock' %}
        {% include '_components/heading-block' with { props: block } only %}
      {% case 'socialLinksBlock' %}
        {% include '_components/social-links-block' with { props: block } only %}
    {% endswitch %}
  {% endif %}
{% endfor %}
